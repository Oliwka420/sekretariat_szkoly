                        <TextBox Width="100px"  FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Left" Name="PracownikImie"/>
                        <Label Content="Drugie imie: " FontSize="15"/>
                        <TextBox Width="100px"  FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Left" Name="Pracownik2Imie"/>
                        <Label Content="nazwisko: " FontSize="15"/>
                        <TextBox Width="100px"  FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Left" Name="PracownikNazwisko"/>
                        <Label Content="nazwisko panieńskie: " FontSize="15"/>
                        <TextBox Width="100px"  FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Left" Name="Pracownik2Nazwisko"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Grid.Row="2" VerticalAlignment="Center">
                        <StackPanel Orientation="Vertical">
                            <Label Content="Imie matki: " FontSize="15"/>
                            <Label Content="Imie ojca: " FontSize="15" Margin="0,-16,0,0"/>
                        </StackPanel>
                        <StackPanel Orientation="Vertical">
                            <TextBox Width="100px"  FontSize="10" Margin="0,7,0,0" Name="PracownikMatka"/>
                            <TextBox Width="100px"  FontSize="10" Margin="0,3,0,0" Name="PracownikOjciec"/>
                        </StackPanel>
                        
                        
                        <Label Content="pesel: " FontSize="15"/>
                        <TextBox Width="100px"  FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Left" Name="PracownikPesel"/>
                        <Label Content="data urodzenia: " FontSize="15" VerticalAlignment="Center"/>
                        <DatePicker FontSize="10" Name="Pracownik_urodzenie" VerticalAlignment="Center"/>
                        <Button Content="zdjecie" Click="przesylanie_zdjecia_pracownik"/>
                        <Button Content="dodaj" Click="dodanie_pracownika"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Grid.Row="3" VerticalAlignment="Center">
                        <Label Content="data zatrudnienia: " FontSize="15"/>
                        <DatePicker FontSize="10" Name="Pracownikzatr"/>
                        <Label Content="plec: " FontSize="15"/>
                        <ComboBox Name="PracowniklPlec" FontSize="15">
                            <ComboBoxItem Content="mezczyzna"/>
                            <ComboBoxItem Content="kobieta" IsSelected="True"/>
                        </ComboBox>
                        <Label Content="etat: " FontSize="15"/>
                        <ComboBox Name="Pracownikletat" FontSize="15">
                            <ComboBoxItem Content="cały etat"/>
                            <ComboBoxItem Content="pół etatu" IsSelected="True"/>
                        </ComboBox>
                        <Label Content="opis stanowiska: " FontSize="15"/>
                        <TextBox Width="100px"  FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Left" Name="PracownikOpis"/>
                        
                    </StackPanel>
                </Grid>
                <TabItem.Background>
                    <LinearGradientBrush EndPoint="0,1">
                        <GradientStop Color="#FFF0F0F0"/>
                        <GradientStop Color="#FFFFC4C4"/>
                    </LinearGradientBrush>
                </TabItem.Background>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
